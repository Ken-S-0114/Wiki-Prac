# Uncomment the next line to define a global platform for your project
# platform :ios, '12.1'

target 'Wiki-Prac' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for Wiki-Prac
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'RxBlocking', '~> 5'
  pod 'RxTest', '~> 5'

  target 'Wiki-PracTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
  end

  target 'Wiki-PracUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 5'
    pod 'RxTest', '~> 5'
  end

end
